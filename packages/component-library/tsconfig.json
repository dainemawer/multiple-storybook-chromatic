{
    "compilerOptions": {
        "allowJs": true,
        "declaration": true,
        "emitDeclarationOnly": true,
        "isolatedModules": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "jsx": "preserve"
    },
    "include": [
        "**/*.ts",
        "**/*.tsx",
    ],
    "exclude": [
        "node_modules"
    ]
}